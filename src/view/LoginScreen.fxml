<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="252.0" prefWidth="437.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="controller.LoginScreenController">
   <children>
      <Label fx:id="loginScreenTitle" layoutX="14.0" layoutY="3.0" text="Chesser Application Management Application (CAPA)" textFill="#2964e4">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="usernameLabel" layoutX="55.0" layoutY="89.0" text="Username:" />
      <Label fx:id="passwordLabel" layoutX="55.0" layoutY="135.0" text="Password:" />
      <TextField fx:id="usernameTextField" layoutX="129.0" layoutY="84.0" />
      <TextField fx:id="passwordTextField" layoutX="129.0" layoutY="130.0" />
      <Button fx:id="loginButton" layoutX="297.0" layoutY="205.0" minWidth="55.0" mnemonicParsing="false" onAction="#loginButtonHandler" text="Login" />
      <Button fx:id="exitButton" layoutX="364.0" layoutY="205.0" minWidth="55.0" mnemonicParsing="false" onAction="#exitButtonHandler" text="Exit" />
   </children>
</AnchorPane>
