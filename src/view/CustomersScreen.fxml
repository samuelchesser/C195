<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="484.0" prefWidth="563.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CustomersScreenController">
   <children>
      <Label fx:id="customersScreenTitle" layoutY="14.0" text="CAPA: Customers Overview " textFill="#2964e4">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TableView fx:id="customersTableView" layoutX="7.0" layoutY="49.0" prefHeight="331.0" prefWidth="546.0">
         <columns>
            <TableColumn fx:id="nameColumn" prefWidth="124.0" text="Name" />
            <TableColumn fx:id="addressColumn" prefWidth="167.0" text="Address" />
            <TableColumn fx:id="cityColumn" prefWidth="91.0" text="City" />
            <TableColumn fx:id="zipCOlumn" prefWidth="75.0" text="Zip" />
            <TableColumn fx:id="phoneColumn" prefWidth="88.0" text="Phone" />
         </columns>
      </TableView>
      <Button layoutX="372.0" layoutY="400.0" minWidth="55.0" mnemonicParsing="false" onAction="#showAddCustomerScreen" text="Add" />
      <Button layoutX="433.0" layoutY="400.0" minWidth="55.0" mnemonicParsing="false" onAction="#showModifyCustomerScreen" text="Modify" />
      <Button layoutX="494.0" layoutY="400.0" minWidth="55.0" mnemonicParsing="false" onAction="#deleteCustomer" text="Delete" />
      <Button layoutX="494.0" layoutY="443.0" minWidth="55.0" mnemonicParsing="false" onAction="#showAppointmentScreen" text="Back" />
   </children>
</AnchorPane>
